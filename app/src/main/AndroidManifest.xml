    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:exported="true"
        android:icon="@drawable/test"
        android:label="@string/app_name"
        android:roundIcon="@drawable/test"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:name">
        <activity android:name="com.mm131.ImgActivity"></activity>
        <activity android:name="com.mm131.MM131Activity" />
        <activity android:name=".kotlin.KotlinActivity" />
        <activity android:name=".rxjava.RxjavaActivity" />
        <activity android:name=".retrofit.RetrofitActivity" />
        <activity android:name=".mvp.MVPActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Main2Activity" />
        <activity android:name=".activity.MarqueeActivity" />
        <activity android:name=".cache.MainActivity3" />
        <activity android:name=".cache.AboutActivity" />
        <activity android:name=".cache.SaveBitmapActivity" />
        <activity android:name=".cache.SaveJsonObjectActivity" />
        <activity android:name=".cache.SaveObjectActivity" />
        <activity android:name=".cache.SaveStringActivity" />
        <activity android:name=".cache.SaveMediaActivity" />
        <activity android:name=".cache.SaveDrawableActivity" />
        <activity android:name=".cache.SaveJsonArrayActivity" />
        <activity android:name=".activity.CameraActivity" />
        <activity android:name=".activity.Main4Activity" />
        <activity android:name=".activity.Main5Activity" />
        <activity android:name=".activity.BigImageActivity" />
        <activity android:name=".activity.ImageActivity" />
        <activity android:name=".activity.Main6Activity" />
        <activity android:name=".activity.LoadingActivity" />
        <activity android:name=".activity.IndicatorActivity" />
        <activity android:name=".activity.SampleActivity" />
        <activity android:name=".activity.SqliteActivity" />
        <activity android:name=".activity.TreeListActivity" />
        <activity android:name=".activity.SendActivity" />
        <activity android:name=".activity.ProgressActivity" />

        <receiver android:name=".NewAppWidget">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/new_app_widget_info" />
        </receiver>

        <activity android:name=".activity.CheckListActivity" />
        <activity android:name=".activity.TableActivity" />
        <activity android:name=".activity.FPSGetActivity" />
        <activity android:name=".activity.MainGetFPSActivity" />

        <service
            android:name=".service.FPSGetService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".activity.NewTableActivity" />
        <activity android:name=".activity.LineChartActivity" />
        <activity android:name=".activity.Main7Activity" />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.AnyOfficeActivity" /> <!-- 登录界面（统一认证需要） -->
        <activity
            android:name="com.huawei.anyoffice.sdk.ui.LoginAuthActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize" /> <!-- 登录配置界面（统一认证需要） -->
        <activity
            android:name="com.huawei.anyoffice.sdk.ui.LoginConfigActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize" /> <!-- SDK中感知终端网络变更（统一认证需要） -->
        <receiver android:name="com.huawei.anyoffice.sdk.network.NetworkReceiver">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver> <!-- SDK中感知锁屏事件，解锁后探测重连隧道功能 -->
        <receiver android:name="com.huawei.anyoffice.sdk.network.ScreenOnReceiver">
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver> <!-- webapp界面（webapp控件需要） -->
        <activity android:name="com.huawei.anyoffice.sdk.ui.WebAppActivity" /> <!-- rar文档界面（打开文档需要） -->
        <activity android:name="com.huawei.anyoffice.sdk.doc.ui.RarViewer" /> <!-- 文档浏览界面（打开文档需要） -->
        <activity
            android:name="com.huawei.anyoffice.sdk.doc.ui.SDKDocViewer"
            android:label="@string/app_name" />
        <activity android:name="com.huawei.AppVpnBaseActivity" />
        <activity android:name=".activity.AnyOfficeActivity2" />
        <activity android:name=".activity.RecycleViewActivity" />
        <activity android:name=".activity.FragmentActivity" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.matrix.myapplication.fileProvider"
            android:exported="false"
            android:grantUriPermissions="true"
            tools:replace="android:authorities">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>


        <!-- JPush -->
        <activity
            android:name=".jpushdemo.MainActivity" >
        </activity>
        <!-- For test only 测试高级功能 -->
        <activity android:name=".jpushdemo.PushSetActivity" android:label="@string/app_name"></activity>
        <!-- For test only 测试设置 -->
        <activity android:name=".jpushdemo.SettingActivity" android:label="@string/app_name"></activity>
        <!-- For test only 测试状态通知栏，需要打开的Activity -->
        <activity android:name=".jpushdemo.TestActivity" android:exported="false">
            <intent-filter>
                <action android:name="jpush.testAction" />
                <category android:name="jpush.testCategory" />
            </intent-filter>
        </activity>

        <!-- Rich push 核心功能 since 2.0.6-->
        <activity
            android:name="cn.jpush.android.ui.PopWinActivity"
            android:theme="@style/MyDialogStyle"
            android:exported="false">
        </activity>

        <!-- Required SDK核心功能-->
        <activity
            android:name="cn.jpush.android.ui.PushActivity"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.NoTitleBar"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.ui.PushActivity" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.matrix.myapplication" />
            </intent-filter>
        </activity>

        <!-- Required SDK 核心功能-->
        <!-- 可配置android:process参数将PushService放在其他进程中 -->
        <service
            android:name="cn.jpush.android.service.PushService"
            android:process=":pushcore"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTER" />
                <action android:name="cn.jpush.android.intent.REPORT" />
                <action android:name="cn.jpush.android.intent.PushService" />
                <action android:name="cn.jpush.android.intent.PUSH_TIME" />
            </intent-filter>
        </service>
        <!-- since 3.0.9 Required SDK 核心功能-->
        <provider
            android:authorities="com.matrix.myapplication.DataProvider"
            android:name="cn.jpush.android.service.DataProvider"
            android:process=":pushcore"
            android:exported="false"
            />

        <!-- since 1.8.0 option 可选项。用于同一设备中不同应用的JPush服务相互拉起的功能。 -->
        <!-- 若不启用该功能可删除该组件，将不拉起其他应用也不能被其他应用拉起 -->
        <service
            android:name="cn.jpush.android.service.DaemonService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.DaemonService" />
                <category android:name="com.matrix.myapplication" />
            </intent-filter>

        </service>
        <!-- since 3.1.0 Required SDK 核心功能-->
        <provider
            android:authorities="com.matrix.myapplication.DownloadProvider"
            android:name="cn.jpush.android.service.DownloadProvider"
            android:exported="true"
            />
        <!-- Required SDK核心功能-->
        <receiver
            android:name="cn.jpush.android.service.PushReceiver"
            android:enabled="true"
            android:exported="false">
            <intent-filter android:priority="1000">
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED_PROXY" />   <!--Required  显示通知栏 -->
                <category android:name="com.matrix.myapplication" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <!-- Optional -->
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <!-- Required SDK核心功能-->
        <receiver android:name="cn.jpush.android.service.AlarmReceiver" android:exported="false"/>

        <!--since 3.3.0 Required SDK核心功能-->
        <activity
            android:name="cn.jpush.android.service.JNotifyActivity"
            android:exported="true"
            android:taskAffinity="jpush.custom"
            android:theme="@android:style/Theme.Translucent.NoTitleBar">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.JNotifyActivity" />
                <category android:name="com.matrix.myapplication" />
            </intent-filter>
        </activity>

        <!-- Since JCore2.0.0 Required SDK核心功能-->
        <!-- 这个Service要继承JCommonService -->
        <service android:name=".jpushdemo.PushService"
            android:process=":pushcore">
            <intent-filter>
                <action android:name="cn.jiguang.user.service.action" />
            </intent-filter>
        </service>


        <!-- User defined.  For test only  用户自定义的广播接收器-->
        <receiver
            android:name=".jpushdemo.MyReceiver"
            android:exported="false"
            android:enabled="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTRATION" /> <!--Required  用户注册SDK的intent-->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" /> <!--Required  用户接收SDK消息的intent-->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" /> <!--Required  用户接收SDK通知栏信息的intent-->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" /> <!--Required  用户打开自定义通知栏的intent-->
                <action android:name="cn.jpush.android.intent.CONNECTION" /><!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <category android:name="com.matrix.myapplication" />
            </intent-filter>
        </receiver>

        <!-- User defined.  For test only  用户自定义接收消息器,3.0.7开始支持,目前新tag/alias接口设置结果会在该广播接收器对应的方法中回调-->
        <receiver android:name=".jpushdemo.MyJPushMessageReceiver">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.RECEIVE_MESSAGE" />
                <category android:name="com.matrix.myapplication"></category>
            </intent-filter>
        </receiver>
        <!-- Required  . Enable it you can get statistics data with channel -->
        <meta-data android:name="JPUSH_CHANNEL" android:value="developer-default"/>
        <meta-data android:name="JPUSH_APPKEY" android:value="0e55e1dd2704052c1f3119ad" /> <!--  </>值来自开发者平台取得的AppKey-->


    </application>

</manifest>